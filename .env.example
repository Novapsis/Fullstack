# ==============================================
# CONFIGURACIÓN N8N AUTOMATION STACK
# ==============================================

# Configuración PostgreSQL Principal
POSTGRES_USER=n8n_admin
POSTGRES_PASSWORD=TU_PASSWORD_POSTGRESQL_MUY_SEGURO
POSTGRES_DB=n8n_database
POSTGRES_NON_ROOT_USER=n8n_user
POSTGRES_NON_ROOT_PASSWORD=TU_PASSWORD_USUARIO_SEGURO

# Configuración N8N Core
N8N_BASIC_AUTH_ACTIVE=true
TIMEZONE=Europe/Madrid

# Configuración Supabase
SUPABASE_DB_PASSWORD=TU_PASSWORD_SUPABASE_SEGURO
SUPABASE_JWT_SECRET=TU_JWT_SECRET_SUPABASE_MUY_SEGURO_32_CHARS

# Configuración de Logs y Ejecuciones
EXECUTIONS_DATA_MAX_AGE=168
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console

# Nota: Coolify generará automáticamente estas variables:
# - SERVICE_FQDN_N8N (tu dominio)
# - SERVICE_USER_N8N (usuario aleatorio)
# - SERVICE_PASSWORD_N8N (contraseña aleatoria)
# - SERVICE_PASSWORD_64_N8N (clave de encriptación)
